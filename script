submitButton.onclick = function Validate(){
    
    
    var firstName = document.getElementById("firstNameTextBox").value;
    var lastName = document.getElementById("lastNameTextBox").value;
    var pnum = document.getElementById("phnumBox").value;
    var dob = document.getElementById("dobBox").value;
    if(firstName == ''){
        alert("First Name is Required");
        return;
    };
    if(lastName == ''){    
       alert("Last Name Required"); 
       return;
    };    
    if( pnum == ''){
     alert("Mobile Number is Required");
     return;
     };     
     if( dob == ''){
     alert("Date of Birth is Required");
     return;    
    };
    if(firstName !='' && lastName != '' && pnum != '' && dob != ''){
    console.log("Sample Output : ")
    console.log("First Name : " +firstName);
    console.log("Last Name : " +lastName);
    console.log("Mobile Number : " +pnum);
    console.log("Date of Birth : " +dob);
    };
};
